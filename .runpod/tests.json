{
  "tests": [
    {
      "name": "basic_generation",
      "input": {
        "model": "openai/gpt-oss-120b",
        "input": [
          { "role": "user", "content": "Explain MXFP4 in one sentence." }
        ],
        "max_output_tokens": 64
      },
      "timeout": 120000
    }
  ],
  "config": {
    "gpuTypeId": "NVIDIA H100 80GB",
    "gpuCount": 1,
    "env": [
      { "key": "ENGINE", "value": "max" },
      { "key": "MODEL_ID", "value": "openai/gpt-oss-120b" },
      { "key": "HF_HOME", "value": "/models/hf_cache" }
    ],
    "allowedCudaVersions": [
      "12.8", "12.7", "12.6", "12.5", "12.4",
      "12.3", "12.2", "12.1", "12.0"
    ]
  }
}